{
    "ban": {
        "name": "ban",
        "group": "admin",
        "desc": "Ban an user from the server",
        "usage": ".ban [user] [reason]",
        "example": ".ban @example#1234 blah blah"
    },
    "banlist": {
        "name": "banlist",
        "group": "admin",
        "desc": "Get list of banned users",
        "usage": ".banlist [number]",
        "example": ".banlist 1"
    },
    "eval": {
        "name": "eval",
        "group": "admin",
        "desc": "Eval command for javascript",
        "usage": ".eval [js]",
        "example": ".eval message.delete()"
    },
    "purgebulk": {
        "name": "purgebulk",
        "group": "admin",
        "desc": "Delete an amount of messages, the number must be between 1 and 100",
        "usage": ".purgebulk [number]",
        "example": ".purgebulk 5"
    },
    "kick": {
        "name": "kick",
        "group": "admin",
        "desc": "Kick an user from the server",
        "usage": ".kick [user] [reason]",
        "example": ".kick @example#1234 blah blah"
    },
    "help": {
        "name": "help",
        "group": "info",
        "desc": "Display the list of commands if you just type .help or if you type .help commandName display the current command info",
        "usage": ".help or .help [commandName]",
        "example": ".help or .help getroles"
    },
    "info": {
        "name": "info",
        "group": "info",
        "desc": "Get bot info",
        "usage": ".info",
        "example": ".info"
    },
    "serverinfo": {
        "name": "serverinfo",
        "group": "info",
        "desc": "Get server info",
        "usage": ".serverinfo",
        "example": ".serverinfo"
    },
    "uptime": {
        "name": "uptime",
        "group": "info",
        "desc": "Get bot uptime",
        "usage": ".uptime",
        "example": ".uptime"
    },
    "userinfo": {
        "name": "userinfo",
        "group": "info",
        "desc": "Get your user info",
        "usage": ".userinfo",
        "example": ".userinfo"
    },
    "ping": {
        "name": "ping",
        "group": "admin",
        "desc": "Ping the bot",
        "usage": ".ping",
        "example": ".ping"
    },
    "avatar": {
        "name": "avatar",
        "group": "info",
        "desc": "Diplay the avatar of a given user",
        "usage": ".avatar [user]",
        "example": ".avatar @example#1234"
    },
    "getroles": {
        "name": "getroles",
        "group": "info",
        "desc": "Diplay the roles of a given user",
        "usage": ".getroles [user]",
        "example": ".getroles @example#1234"
    }
}